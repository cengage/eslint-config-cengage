{
  "name": "eslint-config-cengage",
  "version": "2.1.3",
  "description": "Base eslint config for use in JavaScript projects at Cengage",
  "files": [
    "base.js",
    "index.js",
    "react.js"
  ],
  "engines": {
    "node": "^10.12.0 || >=12.0.0"
  },
  "keywords": [
    "eslint",
    "eslintconfig",
    "cengage"
  ],
  "scripts": {
    "cm": "git-cz",
    "cm-retry": "git-cz --retry"
  },
  "contributors": [
    {
      "name": "Gillett, Ryan C",
      "email": "ryan.gillett@cengage.com"
    },
    {
      "name": "Maturen, Aaron T",
      "email": "aaron.maturen@cengage.com"
    },
    {
      "name": "Van Slaars, Andy L",
      "email": "andy.vanslaars@cengage.com"
    }
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:cengage/eslint-config-cengage.git"
  },
  "license": "ISC",
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@typescript-eslint/eslint-plugin": "^4.2.0",
    "@typescript-eslint/parser": "^4.2.0",
    "babel-eslint": "^10.1.0",
    "commitizen": "^4.2.2",
    "conventional-changelog-cli": "^2.1.1",
    "conventional-changelog-conventionalcommits": "^4.5.0",
    "conventional-changelog-core": "^4.2.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.9.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^24.0.2",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-react-hooks": "^4.1.2",
    "eslint-plugin-unicorn": "^22.0.0",
    "husky": "^4.3.0",
    "typescript": "^3.9.4"
  },
  "peerDependencies": {
    "babel-eslint": "^10.1.0",
    "eslint": "^7.9.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^24.0.2",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "7.20.6",
    "eslint-plugin-react-hooks": "^4.1.2",
    "eslint-plugin-unicorn": "^22.0.0",
    "@typescript-eslint/eslint-plugin": "^4.2.0",
    "@typescript-eslint/parser": "^4.2.0",
    "typescript": "^3.9.4"
  },
  "dependencies": {
    "confusing-browser-globals": "^1.0.9"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}